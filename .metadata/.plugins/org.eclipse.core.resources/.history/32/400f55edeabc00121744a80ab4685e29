package com.undeadstudio.returners.screens;

import com.badlogic.gdx.scenes.scene2d.ui.TextButton;

public class GameOverScreen extends AbstractMenuScreen {

	float columns, rows;
	TextButton[][] buttonGrid;
	String[] labels;

	public GameOverScreen(String title, int columns, int rows, String[] labels) {
		super(title, columns, rows, labels);
		this.columns = columns;
		this.rows = rows;
		buttonGrid = new TextButton[columns][rows];
		this.labels = labels;
	}

}
